  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Rails",
      "type": "Ruby",
      "request": "attach",
      "remotePort": "1234",
      "remoteHost": "127.0.0.1",
      "remoteWorkspaceRoot": "/api",
      "cwd": "${workspaceRoot}",
      "showDebuggerOutput": true
    },
    {
      "name": "Debug Rails",
      "type": "Ruby",
      "request": "attach",
      "remotePort": "1234",
      "remoteHost": "127.0.0.1",
      "remoteWorkspaceRoot": "/api",
      "cwd": "${workspaceRoot}",
      "showDebuggerOutput": true,
      "preLaunchTask": "debug",
      "postDebugTask": "stop debug"
    },
  ]
}
